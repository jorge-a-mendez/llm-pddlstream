(define (stream kuka-tamp)
  (:stream sample-pose
    :inputs (?o ?s)
    :domain (Stackable ?o ?s)
    :outputs (?p)
    :certified (and (Pose ?o ?p) (Supported ?o ?p ?s))
  )
  (:stream sample-grasp
    :inputs (?o)
    :domain (Graspable ?o)
    :outputs (?g)
    :certified (Grasp ?o ?g)
  )
  (:stream inverse-kinematics
    :inputs (?r ?o ?p ?g)
    :domain (and (Robot ?r) (Pose ?o ?p) (Grasp ?o ?g))
    :outputs (?q ?t)
    :certified (and (Conf ?r ?q) (Traj ?r ?t)
                    (Kin ?r ?o ?p ?g ?q ?t))
  )
  (:stream plan-motion
    :inputs (?r ?q1 ?q2)
    :domain (and (Conf ?r ?q1) (Conf ?r ?q2) (Robot ?r))
    ;:fluents (AtPose AtConf)
    :outputs (?t)
    :certified (Motion ?r ?q1 ?q2 ?t)
  )

  ;(:predicate (TrajCollision ?r ?t ?o2 ?p2)
  ;  (and (Traj ?r ?t) (Pose ?o2 ?p2))
  ;)
)